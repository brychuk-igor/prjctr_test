.list
	&__container
		margin 0 auto
		padding-bottom 10px
		max-width 1200px
		display -webkit-flex
		display -moz-flex
		display -ms-flex
		display -o-flex
		display flex
		flex-wrap wrap
		justify-content space-between
		align-items center
		@media xs
			justify-content center
			padding-bottom 10px
		@media sm
			justify-content space-around
			padding 0 15px
			padding-bottom 10px
		@media md
			justify-content space-around
			padding 0 15px
			padding-bottom 10px
		@media lg
			justify-content space-around
			padding 0 15px
			padding-bottom 10px
		@media xl
			justify-content space-between
			padding 0px
			padding-bottom 10px	
	&__item
		flex-basis 280px
		margin 0 0px 30px 0px
		text-decoration none
		
